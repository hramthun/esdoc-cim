	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="urn:x-auscope:xmlns:CSIRO:FullMoon:package-register:2.0"
	xsi:schemaLocation="urn:x-auscope:xmlns:CSIRO:FullMoon:package-register:2.0 ../dependency-util/package-register.xsd"
	id="cim_local_PackageRegister.xml">
	<description>A register of additional EA packages with classmap implementations for HMA		
